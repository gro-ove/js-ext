	// 12.1 Block
	function parseStatementList (){
		var list = [],
			statement;

		while (index < length){
			if (match ('}'))
				break;
			
			statement = parseSourceElement ();

			if (typeof statement === 'undefined')
				break;

			list.push (statement);
		}

		return list;
	}

	function parseBlock (){
		var block;

		expect ('{');

		block = parseStatementList ();

		expect ('}');

		return {
			type: 	Syntax.BlockStatement,
			body: 	block
		};
	}