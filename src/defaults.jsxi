	function paramsManager (){
		var that = this;

		return { 
			add: lambda (key, value){ 
				switch (key){
					case 'import':
						var other = that.context.file.find (value);
						if (other)
							other.forEach (lambda arg.process ());
						else
							throw new MacroError (that.name, 'Importing file "' + value + '" not found');
						break;
					default:
						throw new MacroError (that.name, 'Wrong param key ("' + key + '")');
				}
			} 
		}
	}